    def _log(self, msg: str, also_ui: bool = False):
        try:
            ts = datetime.now().strftime("%H:%M:%S")
            line = f"{ts} {msg}"
            self._log_buffer.append(line)
            if len(self._log_buffer) > 300:
                self._log_buffer = self._log_buffer[-200:]
            log(f"{_TAG} {msg}")
            if also_ui:
                self._notify(f"{_TAG} {msg}")
        except Exception:
            pass
