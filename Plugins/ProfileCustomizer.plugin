                except Exception:
                    pass
                try:
                    BooleanTYPE = find_class("java.lang.Boolean").TYPE
                    m2 = ThemeClass.getDeclaredMethod("setColor", IntegerTYPE, IntegerTYPE, BooleanTYPE)
                    m2.setAccessible(True)
                    m2.invoke(None, int(ikey), int(color), False)
                except Exception:
                    pass
        except Exception:
            pass
