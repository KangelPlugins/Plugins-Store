    def _invalidate_ui(self):
        try:
            frag = self._safe_get_fragment()
            if frag is None:
                frag = get_last_fragment()
            if frag is not None:
                view = None
                try:
                    view = frag.getFragmentView()
                except Exception:
                    pass
                if view is not None:
                    try:
                        view.invalidate()
                        from android.view import ViewGroup as JVG
                        if isinstance(view, JVG):
